    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.3.1",
    "@types/node": "^24",
    "jsdom": "^27.4.0",
    "svelte": "^5.43.8",
    "svelte-check": "^4.3.4",
