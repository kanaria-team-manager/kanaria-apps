{
  "name": "@kanaria/backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --minify",
    "deploy:prod": "env $(grep -v '^#' ./.dev.vars.production | grep -v '^$' | xargs) wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check --error-on-warnings",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:generate:dml": "drizzle-kit generate --custom",
    "drizzle:push": "env $(grep -v '^#' ./.dev.vars | grep -v '^$' | xargs) drizzle-kit push",
    "drizzle:pull": "env $(grep -v '^#' ./.dev.vars | grep -v '^$' | xargs) drizzle-kit pull",
    "drizzle:drop": "env $(grep -v '^#' ./.dev.vars | grep -v '^$' | xargs) drizzle-kit drop",
    "drizzle:migrate": "env $(grep -v '^#' ./.dev.vars | grep -v '^$' | xargs) drizzle-kit migrate",
    "drizzle:push:prod": "env $(grep -v '^#' ./.dev.vars.production | grep -v '^$' | xargs) drizzle-kit push",
    "seed:players": "env $(grep -v '^#' ./.dev.vars | grep -v '^$' | xargs) tsx scripts/generate-dummy-players.ts",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:status": "supabase status"
  },
  "packageManager": "pnpm@10.20.0",
  "dependencies": {
    "@hono/zod-validator": "^0.7.5",
    "@kanaria/shared": "workspace:*",
    "@supabase/supabase-js": "^2.86.0",
    "drizzle-orm": "^0.44.7",
    "hono": "^4.10.6",
    "jose": "^6.1.3",
    "postgres": "^3.4.7",
    "ulid": "^3.0.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@types/node": "^24.10.1",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.7",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14",
    "wrangler": "^4.4.0"
  }
}
